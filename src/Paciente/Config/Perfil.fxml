<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="410.0" prefWidth="587.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Paciente.Config.PerfilController">
   <children>
      <HBox alignment="CENTER">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Paciente">
               <font>
                  <Font name="SansSerif Bold" size="14.0" />
               </font>
            </Text>
         </children>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nome" />
            <TextField fx:id="t1" prefHeight="30.0" prefWidth="400.0">
               <HBox.margin>
                  <Insets left="29.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Sobrenome" />
            <TextField fx:id="t2" prefHeight="30.0" prefWidth="400.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="RG" />
            <TextField fx:id="t3" editable="false" prefHeight="30.0" prefWidth="400.0">
               <HBox.margin>
                  <Insets left="46.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="CPF" />
            <TextField fx:id="t4" editable="false" prefHeight="30.0" prefWidth="400.0">
               <HBox.margin>
                  <Insets left="41.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="E-mail" />
            <TextField fx:id="t5" prefHeight="30.0" prefWidth="400.0">
               <HBox.margin>
                  <Insets left="28.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Estado" />
            <ComboBox fx:id="comboEst" prefHeight="25.0" prefWidth="400.0" promptText="&lt;Selecione seu Estado&gt;">
               <HBox.margin>
                  <Insets left="26.0" />
               </HBox.margin>
            </ComboBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" spacing="70.0">
         <children>
            <Button fx:id="delete" mnemonicParsing="false" onAction="#deletar" text="Deletar Conta" />
            <Button fx:id="save" mnemonicParsing="false" onAction="#salvar" text="Salvar" />
         </children>
      </HBox>
   </children>
</VBox>
